<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p %C{1} (%F:%L) - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<!-- 那个包日志级别 -->
		<Logger name="com.yc" level="debug" />

		<!-- 总日志级别 -->
		<Root level="error">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>
</Configuration>